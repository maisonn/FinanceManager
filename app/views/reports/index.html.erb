<div class="container">
<p id="notice"><%= notice %></p>

<%= render 'reportstitle' %>

<%= render 'form' %>
</div>

<div class="container-fluid">
    <% if @start_date.nil? || @end_date.nil? || params[:types].nil? %>
        <% elsif (@start_date <= @end_date) && (@start_date != '' || @end_date != '') && (params[:types] == 'Expenses') %>
            <%= render 'expenseitems' %>
        <% elsif (@start_date <= @end_date) && (@start_date != '' || @end_date != '') && (params[:types] == 'Incomes') %>
            <%= render 'incomeitems' %>
        <% else %>
    <% end %>
</div>

<figure class="highcharts-figure">
  <div id="container"></div>
  <p class="highcharts-description">
    All color options in Highcharts can be defined as gradients or patterns.
    In this chart, a gradient fill is used for decorative effect in a pie
    chart.
  </p>
</figure>